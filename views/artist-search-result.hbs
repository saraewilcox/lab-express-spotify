<div class="container">
    <div class="row">
        {{#each result}}
        <div class="artist-result">
            <img src={{this.images.[0].url}} alt="artist-pics">
            <h3>{{this.name}}</h3>
            <a href="/albums/{{this.id}}">View Albums</a>
        </div>
        {{/each}}
    </div>
</div>